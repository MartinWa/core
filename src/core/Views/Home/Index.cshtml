@model IEnumerable<core.Models.Resturant>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Home</title>
</head>
<body>
<h1>Welcome</h1>
<div>
    <table>
        @foreach (var resturant in Model)
        {
            <tr>
                <td>@resturant.Id</td>
                <td>@resturant.Name</td>
            </tr>
        }
    </table>
</div>
</body>
</html>
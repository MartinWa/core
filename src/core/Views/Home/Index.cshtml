@model core.ViewModels.HomeViewModel
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Home</title>
</head>
<body>
    <h1>Welcome</h1>
    <h3>@Model.CurrentGreeting</h3>
    <div>
        <table>
            @foreach (var resturant in Model.Resturants)
            {
                <tr>
                    <td>@Html.ActionLink(resturant.Id.ToString(), "Details", new { id = resturant.Id })</td>
                    <td>@resturant.Name</td>
                </tr>
            }
        </table>
    </div>
</body>
</html>